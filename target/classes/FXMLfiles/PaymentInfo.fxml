   <children>
      <RadioButton fx:id="paymentTypeButtonCash" layoutX="84.0" layoutY="60.0" mnemonicParsing="false" text="Cash" />
      <RadioButton fx:id="paymentTypeButtonCard" layoutX="84.0" layoutY="91.0" mnemonicParsing="false" text="Debit/Credit" />
      <RadioButton fx:id="paymentTypeButtonCheck" layoutX="84.0" layoutY="120.0" mnemonicParsing="false" text="Check" />
      <Label layoutX="16.0" layoutY="14.0" text="Choose Payment:" underline="true">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button layoutX="274.0" layoutY="339.0" mnemonicParsing="false" onAction="#PaymentType" text="continue" />
   </children>
</AnchorPane>
